{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python",
      "config": {
        "installCommand": "pip install -r requirements.txt",  
        "buildCommand": "",  
        "devCommand": "flask run",  
        "outputDirectory": ""  
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ]
}
